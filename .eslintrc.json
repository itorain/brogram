{
    "rules": {
        "indent": [
            1,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "object-curly-spacing": "off"
    },
    "env": {
        "es6": true,
        "node": true
    },
    "plugins": [
        "react"
    ],
    "extends": "google"
}
